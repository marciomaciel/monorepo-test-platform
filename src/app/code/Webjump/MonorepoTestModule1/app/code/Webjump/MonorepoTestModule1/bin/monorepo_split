#!/usr/bin/env bash

if [[ ! -f ~/monorepo-tools/monorepo_split.sh ]]; then
  echo "File not found: ~/monorepo-tools/monorepo_split.sh"
  exit 1
fi

~/monorepo-tools/monorepo_split.sh \
    module-email-logger:app/code/Webjump/EmailLogger \
    module-password-validation:app/code/Webjump/PasswordValidation \
    module-payment-notifier:app/code/Webjump/PaymentNotifier \
    module-sales-review:app/code/Webjump/SalesReview